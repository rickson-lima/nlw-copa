generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pool {
  id        String   @id @default(cuid()) // primay and autogenerate key
  title     String
  code      String   @unique
  createdAt DateTime @default(now()) // now() = current timestamps
}

